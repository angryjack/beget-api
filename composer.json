{
    "name": "askja/beget-api-fork",
    "type": "library",
    "description": "A PHP library for using Beget's API",
    "keywords": [
        "beget-api", "beget-php-library"
    ],
    "homepage": "https://github.com/ex-Askja/beget-api",
    "license": "MIT",
    "authors": [
        {
            "name": "angryjack",
            "email": "angryj2ck@gmail.com",
            "homepage": "https://github.com/angryjack",
            "role": "Developer"
        },
        {
            "name": "askja",
            "email": "ex.sirenia@gmail.com",
            "homepage": "https://github.com/ex-askja",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Angryjack\\Beget\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
